<menuitem type="menuitem" name="unnamed" scale_effect="no" imageN ="images/laboratory/ui_panel1.png" sound="##sound_button##">
  <children>
    <node name="conteiner" type="node" pos="140x229">
      <children>
        <node name="name" type="text" font="fonts/whitefont1.fnt" text="Tower name" pos="0x-10" color="000000" scale="0.55x0.55"/>
        <node name="icon" type="sprite" pos="0x90"/>
        <node name="icon2" type="sprite" pos="-90x190"/>
        <node name="icon_locked" type="sprite" image="images/laboratory/lock_icon.png" pos="0x-110" visible="no"/>

        <node name="menu_info" type="menu" pos="0x0" z="9">
          <children>
            <node type="menuitem" name="info" imageN ="images/laboratory/icon_i.png" pos="90x190" sound="##sound_button##"/>
          </children>
        </node>

        <node name="main" type="node" pos="0x0">
          <children>
            <node name="dmg" type="text" pos="-95x-90" font="fonts/helvetica.fnt" text="ATK:" center="0x0.5" color="000000" scale="0.6x0.6"/>
            <node name="rng" type="text" pos="30x-90" font="fonts/helvetica.fnt" text="RNG:" center="0x0.5" color="000000" scale="0.6x0.6"/>
            <node name="spd" type="text" pos="-40x-122" font="fonts/helvetica.fnt" text="RATE:" center="0x0.5" color="000000" scale="0.6x0.6"/>

            <node name="icondmg" type="sprite" image="images/laboratory/icon_atk.png" pos="-112x-90" />
            <node name="iconrng" type="sprite" image="images/laboratory/icon_rng.png" pos="12x-92" />
            <node name="iconspd" type="sprite" image="images/laboratory/icon_rate.png" pos="-55x-125" />

            <node name="indicator" type="node" pos="0x-53">
              <children>
                <node name="bg" type="sprite" image="images/laboratory/prorressbar_level_1.png"/>
                <node name="1" pos="-72x0" type="sprite" image="images/laboratory/prorressbar_level_3.png"/>
                <node name="2" pos="-37x0" type="sprite" image="images/laboratory/prorressbar_level_2.png"/>
                <node name="3" pos="0x0" type="sprite" image="images/laboratory/prorressbar_level_2.png"/>
                <node name="4" pos="37x0" type="sprite" image="images/laboratory/prorressbar_level_2.png"/>
                <node name="5" pos="72x0" type="sprite" image="images/laboratory/prorressbar_level_3.png" scale="-1x1"/>
                <node name="caption" type="text" font="fonts/helvetica.fnt" text="" pos="0x2" color="FFFFFF" scale="0.7x0.7"/>
              </children>
            </node>

            <node name="menu" type="menu" pos="0x0" z="9">
              <children>
                <node type="menuitem" name="buy" imageN ="images/laboratory/button_upgrade.png" pos="0x-175" visible="yes" sound="##sound_button##">
                  <children>
                    <node name="normal">
                      <children>
                        <node name="text" type="text" font="fonts/mdefensefont.fnt" text="#laboratory_tower_upgrade#" pos="95x42" scale="0.6x0.6"/>
                        <node name="cost" type="text" font="fonts/mdefensefont.fnt" text="999" pos="100x16" scale="0.4x0.4" center="0x0.5"/>
                        <node name="icon" type="sprite" image="images/laboratory/icon_coin.png" pos="85x14"/>
                      </children>
                    </node>
                  </children>
                </node>
              </children>
            </node>

            <node name="menu_confirm" type="menu" pos="0x0" z="9" visible="no">
              <children>
                <node type="menuitem" name="confirm" imageN ="images/laboratory/icon_ok.png" pos="-95x-175" sound="##sound_button##"/>
                <node type="menuitem" name="cancel" imageN ="images/laboratory/icon_no.png" pos="95x-175" sound="##sound_button##"/>
                <node name="text" type="text" font="fonts/mdefensefont.fnt" text="#laboratory_tower_upgrade#" pos="0x-162" scale="0.6x0.6"/>
                <node name="cost" type="text" font="fonts/mdefensefont.fnt" text="999" pos="5x-189" scale="0.4x0.4" center="0x0.5"/>
                <node name="icon" type="sprite" image="images/laboratory/icon_coin.png" pos="-10x-191"/>
              </children>
            </node>

          </children>
        </node>
        
        <node name="info" type="node" pos="0x0" visible="no" >
          <children>
            <node name="text" type="text" font="fonts/helvetica.fnt" text="Tower desc Tower desc Tower desc Tower desc Tower desc Tower desc Tower desc Tower desc " pos="0x-35" color="000000" scale="0.6x0.6" textwidth="250" center="0.5x1"/>
          </children>
        </node>

      </children>
    </node>
  </children>
</menuitem>
                    {
                        "@die": "death"
                    }
                ],
                "readyfire": [
                    {
                        "@charge": "readyfire"
                    },
                    {
                        "@notarget": "move"
                    }
                ],
                "stop": [
                    {
                        "@move": "move"
                    },
                    {
                        "@die": "death"
                    },
                    {
                        "@seetarget": "readyfire"
                    },
                    {
                        "@notarget": "move"
                    }
                ]
            }
        },
        "mover": {
            "params": {
                "allowangles": {
                    "@value": "0,180,90,270"

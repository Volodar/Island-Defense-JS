{
    "tower": {
        "@radius": "150",
        "@name": "jeep",
        "@lifecost": "1",
        "@color_ice": "80FFFF",
        "@unittype": "creep",
        "@health": "85",
        "@exp": "2",
        "@color_electro": "FFFF80",
        "@unitlayer": "earth",
        "@velocity": "120",
        "@color_fire": "FF8080",
        "effects": {
            "positive": {
                "@armor": "6",
                "@fireResist": "9",
                "@damage": "8",
                "@iceResist": "2"
            }
        },
        "extraproperties": {
            "@freezing_scale": "0.35",
            "@freezing_pos": "0x-5",
            "@electro_scale": "1",
            "@electro_pos": "0x30",
            "@electro_size": "big",
            "@fire_scale": "3",
            "@fire_pos": "0x-5"
        },
        "machine_unit": {
            "@start_state": "stop",
            "transitions": {
                "move": [
                    {
                        "@stop": "stop"
                    },
                    {
                        "@die": "death"
                    }
                ],
                "stop": [
                    {
                        "@move": "move"
                    },
                    {
                        "@die": "death"
                    },
                    {
                        "@seetarget": "readyfire"
                    },
                    {
                        "@notarget": "move"
                    }
                ],
                "readyfire": [
                    {
                        "@charge": "readyfire"
                    },
                    {
                        "@notarget": "move"
                    }
                ],
                "death": {
                    "@stop": "stop"
                }
            },
            "params": {
                "state_death": {
                    "@duration": "1"
                },
                "state_readyfire": {
                    "@delay": "1",
                    "@charge_volume": "99999"
                }
            }
        },
        "mover": {
            "params": {
                "allowangles": {
                    "@value": "0,180,90,270"
                },
                "thresold": {
                    "@value": "45"
                }
            }
        },
        "children": {
            "node": [
                {
                    "@visible": "yes",
                    "@type": "sprite",
                    "@name": "skin",
                    "@center": "0.5x0.38"
                },
                {
                    "@visible": "yes",
                    "@image": "jeep.left::jeep_leftright0001.png",
                    "@type": "sprite",
                    "@name": "skin2",
                    "@center": "0.5x0.38"
                },
                {
                    "@opacity": "192",
                    "@visible": "no",
                    "@type": "sprite",
                    "@name": "death",
                    "@center": "0.5x0.4"
                },
                {
                    "@z": "1",
                    "@name": "health_indicator",
                    "@pos": "-18x65"
                }
            ]
        },
        "actions": {
            "action": [
                {
                    "@name": "animateL",
                    "@value": "RepeatForever[Animate[0.5,[folder:jeep.left::,frames:jeep_leftright0001.png,jeep_leftright0002.png,jeep_leftright0003.png,jeep_leftright0004.png,jeep_leftright0005.png,jeep_leftright0006.png]]]"
                },
                {
                    "@name": "animateU",
                    "@value": "RepeatForever[Animate[0.5,[folder:jeep.up::,frames:jeep_up0001.png,jeep_up0002.png,jeep_up0003.png,jeep_up0004.png,jeep_up0005.png,jeep_up0006.png,jeep_up0007.png]]]"
                },
                {
                    "@name": "animateD",
                    "@value": "RepeatForever[Animate[0.5,[folder:jeep.down::,frames:jeep_down0001.png,jeep_down0002.png,jeep_down0003.png,jeep_down0004.png,jeep_down0005.png,jeep_down0006.png,jeep_down0007.png]]]"
                },
                {
                    "@name": "animate_die",
                    "@value": "Animate[1.0,[folder:death::,frames:Death_stage0001.png,Death_stage0002.png,Death_stage0003.png,Death_stage0004.png,Death_stage0005.png,Death_stage0006.png,Death_stage0007.png,Death_stage0008.png,Death_stage0009.png,Death_stage0010.png,Death_stage0011.png,Death_stage0012.png,Death_stage0013.png,Death_stage0014.png,Death_stage0015.png,Death_stage0016.png,Death_stage0017.png,Death_stage0018.png,Death_stage0019.png,Death_stage0020.png,Death_stage0021.png,Death_stage0022.png,Death_stage0023.png,Death_stage0024.png,Death_stage0025.png]]"
                },
                {
                    "@name": "animate_fire",
                    "@value": "Animate[0.7,[folder:jeep.fire::,indexes:jeep_fire_00.png,01,20]]"
                },
                {
                    "@name": "fadeout",
                    "@value": "EaseOut[FadeOut[1],2]"
                }
            ]
        },
        "events": {
            "eventlist": [
                {
                    "@name": "on_die",
                    "playsound": {
                        "@sound": "##sound_jeep_death##"
                    },
                    "stopallaction": {
                        "@target": "skin"
                    },
                    "action": {
                        "@action": "animate_die",
                        "@state": "run",
                        "@target": "skin"
                    }
                },
                {
                    "@name": "on_shoot",
                    "playsound": {
                        "@sound": "##sound_jeep_shoot##"
                    },
                    "stopallaction": {
                        "@target": "skin"
                    },
                    "action": {
                        "@action": "animate_fire",
                        "@state": "run",
                        "@target": "skin"
                    }
                },
                {
                    "@name": "on_rotate180",
                    "setproperty": {
                        "@property": "scale",
                        "@target": "skin",
                        "@value": "-1x1"
                    },
                    "stopallaction": {
                        "@target": "skin"
                    },
                    "action": {
                        "@action": "animateL",
                        "@state": "run",
                        "@target": "skin"
                    }
                },
                {
                    "@name": "on_rotate0",
                    "setproperty": {
                        "@property": "scale",
                        "@target": "skin",
                        "@value": "1x1"
                    },
                    "stopallaction": {
                        "@target": "skin"
                    },
                    "action": {
                        "@action": "animateL",
                        "@state": "run",
                        "@target": "skin"
                    }
                },
                {
                    "@name": "on_rotate90",
                    "stopallaction": {
                        "@target": "skin"
                    },
                    "action": {
                        "@action": "animateD",
                        "@state": "run",
                        "@target": "skin"
                    }
                },
                {
                    "@name": "on_rotate270",
                    "stopallaction": {
                        "@target": "skin"
                    },
                    "action": {
                        "@action": "animateU",
                        "@state": "run",
                        "@target": "skin"
                    }
                }
            ]
        }
    }
}
{
    "tutorial": {
        "@template": "ini/tutorial/units/template.xml",
        "children": {
            "node": [
                {
                    "@path": "body/bgtype",
                    "@image": "images/tutorial/tut5_ground.png"
                },
                {
                    "@path": "body/name",
                    "@text": "#moto_name#"
                },
                {
                    "@path": "body/caption",
                    "@text": "#moto_caption#"
                },
                {
                    "@path": "body/description",
                    "@pos": "-110x-75",
                    "@text": "#moto_desc#"
                },
                {
                    "@path": "body/unit",
                    "@scale": "-1x1",
                    "@animation": "         RepeatForever         [           Sequence           [             ScaleTo[0,-1,1],             Spawn[MoveTo[0.5,-180,40],Animate[0.5,[folder:moto::,indexes:moto_go_00.png,01:06]]],             Animate[0.3,[folder:moto::,indexes:moto_fire_00.png,01:06]],             ScaleTo[0,1,1],             Spawn[MoveTo[0.5,-240,40],Animate[0.5,[folder:moto::,indexes:moto_go_00.png,01:06]]],             Animate[0.3,[folder:moto::,indexes:moto_fire_00.png,01:06]]           ]         ]         "
                }
            ]
        }
    }
}
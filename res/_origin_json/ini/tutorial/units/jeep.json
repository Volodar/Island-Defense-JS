{
    "tutorial": {
        "@template": "ini/tutorial/units/template.xml",
        "children": {
            "node": [
                {
                    "@image": "images/tutorial/tut5_ground.png",
                    "@path": "body/bgtype"
                },
                {
                    "@path": "body/name",
                    "@text": "#jeep_name#"
                },
                {
                    "@path": "body/caption",
                    "@text": "#jeep_caption#"
                },
                {
                    "@path": "body/description",
                    "@text": "#jeep_desc#"
                },
                {
                    "@animation": "         RepeatForever         [           Sequence           [             ScaleTo[0,1,1],             Spawn[MoveTo[0.4,-180,40],Animate[0.6,[folder:jeep.left::,indexes:jeep_leftright00.png,01:06]]],             Animate[0.6,[folder:jeep.fire::,indexes:jeep_fire_00.png,01:20]],             ScaleTo[0,-1,1],             Spawn[MoveTo[0.4,-240,40],Animate[0.6,[folder:jeep.left::,indexes:jeep_leftright00.png,01:06]]],             Animate[0.6,[folder:jeep.fire::,indexes:jeep_fire_00.png,01:20]]           ]         ]         ",
                    "@path": "body/unit"
                }
            ]
        }
    }
}